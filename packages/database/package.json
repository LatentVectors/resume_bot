{
  "name": "@resume/database",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./server": "./src/server-client.ts",
    "./admin": "./src/admin-client.ts",
    "./client": "./src/client.ts",
    "./hooks": "./src/hooks/use-supabase.ts",
    "./types": "./src/types/index.ts"
  },
  "scripts": {
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:reset": "supabase db reset",
    "db:gen-types": "supabase gen types typescript --local > src/types/database.types.ts",
    "migrate:from-sqlite": "dotenv -- npx tsx scripts/migrate-from-sqlite.ts",
    "seed:templates": "npx tsx scripts/seed-templates.ts",
    "db:backup": "mkdir -p data && docker exec supabase_db_resume pg_dump -U postgres -d postgres > data/backup.sql",
    "db:restore": "docker exec -i supabase_db_resume psql -U postgres -d postgres < data/backup.sql"
  },
  "dependencies": {
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.47.12",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^20",
    "dotenv-cli": "^11.0.0",
    "sql.js": "^1.12.0",
    "tsx": "^4.19.2",
    "typescript": "^5"
  }
}
